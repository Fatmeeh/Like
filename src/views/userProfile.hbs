<section class="user-profile">
<div class="user">
<div class="user-img-container">
<img class="user-img" src={{user.img}}>
</div>
<div class="user-info-container">
<h1><label>{{user.name}}</label></h1>
<p>description : {{user.description}}</p>
</div>
</section>

 <svg style="display: none" version="1.1" xmlns="http://www.w3.org/2000/svg" xmlns:xlink="http://www.w3.org/1999/xlink">
    <symbol id="icon-heart" viewBox="0 0 32 32">
      <path d="M16.042 8.345c0 0-2-4.262-6.5-4.262-4.917 0-7.5 4.167-7.5 8.333 0 6.917 14 15.5 14 15.5s13.916-8.5 13.916-15.5c0-4.25-2.666-8.333-7.416-8.333s-6.5 4.262-6.5 4.262z"/>
    </symbol>
  </svg>

<section class="img-container">
     <h2>Your favorite picture</h2>
    <ul>
      <li>
        <div class="image" style="background-image: url({{img.url}})"></div>
        <div class="image_info_container">
          <h2 class="image_info">{{img.info}}</h2>
          <h23 class="image_category">{{img.category}}</h23>
          <svg class="image_fav-btn"><use id="js-fav-btn" xlink:href="#icon-heart"></use></svg>
        </div>
      </li>

    </ul>
  </section>
</div>

</div>